<div-async name="settings-table" id="payments-table">
  <div class="people-table">
    <tutorial step="2">
      <div id="tutorial-2" class="a">
      </div>
    </tutorial>
    <tutorial step="3">
      <div id="tutorial-3" class="b">
      </div>
    </tutorial>
    <result>
      <div class="person $(personClasses)">
        <apply template="settings_show"></apply>
      </div>
    </result>
    <div class="clearfix"/>
  </div>
</div-async>